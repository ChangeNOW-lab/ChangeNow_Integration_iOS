# Uncomment the next line to define a global platform for your project
platform :ios, '11.0'
use_frameworks!

def main_pods
    pod 'Moya'
    pod 'Kingfisher'
    pod 'PocketSVG'
    pod 'R.swift'
    pod 'CocoaLumberjack'
    pod 'SnapKit'
    pod 'NVActivityIndicatorView/Extended'
    pod 'Resolver'
end

########################
##### MAIN TARGETS #####
########################

target 'CNIntegration' do
    main_pods
end

target 'CNIntegrationTests' do
    inherit! :search_paths
end
